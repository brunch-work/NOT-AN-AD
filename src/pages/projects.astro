---
import Layout from "../layouts/Layout.astro";

// Define the type for a project
type Project = {
  client: string;
  service: string;
  publishedAt: string;
  slug: string;
};
---

<Layout>
  <ul>
    {
      projects.map((project: Project) => (
        <li>
          {project.client} | {project.service} | {project.publishedAt}
        </li>
      ))
    }
  </ul>
</Layout>
